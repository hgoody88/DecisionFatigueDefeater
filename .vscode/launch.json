   "version": "0.2.0",
   "configurations": [
      {
         "name": "Run Windows Hermes - Experimental",
         "request": "launch",
         "type": "reactnativedirect",
         "cwd": "${workspaceFolder}",
         "enableDebug": false,
         "platform": "windows"
      },
      {
         "name": "Attach to Hermes application - Experimental",
         "request": "attach",
         "type": "reactnativedirect",
         "cwd": "${workspaceFolder}",
         "port": 19000
      },
      {
         "name": "Debug in Exponent",
         "request": "launch",
         "type": "reactnative",
         "cwd": "${workspaceFolder}",
         "platform": "exponent",
         "expoHostType": "local"
      }
   ]
}
